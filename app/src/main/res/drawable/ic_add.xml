<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <item
        android:height="45dp"
        android:width="50dp">

        <shape>

            <solid
                android:color="#006659">
            </solid>

            <corners
                android:bottomLeftRadius="3dp"
                android:topLeftRadius="3dp">
